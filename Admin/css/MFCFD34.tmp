*{margin: 0; padding: 0;}
body 
{
	margin:0;
	padding:0;
	font-family:Arial, Helvetica, sans-serif;
	background:#FFF;
}
img{ border:none;}
#top_box
{
	width:100%;
	height:180px;
	margin:auto;
	background:url(../images/top_bg.jpg) no-repeat center top;
}
#topp
{
	width:980px;
	height:180px;
	margin:auto;
}
#topp_co
{
	width:980px;
	height:130px;
	float:left;
}
#logo_box
{
	width:200px;
	height:130px;
	float:left;
}#logo_box h1 { padding:20px 0 0 0;}

#logo_bal
{
	width:780px;
	height:50px;
	float:left;
}
#logo_bal h1
{
	font-size:12px;
	font-weight:normal;
	color:#781F0D;
	text-align:right;
	padding:10px 0 0 0;
	float:right;
}

#top_blogs
{
	width:780px;
	height:80px;
	float:left;
}

#top_blogs ul
{
	list-style:none;
	padding:0;
	margin:0 75px 0 0;
}
#top_blogs ul li
{
	float:right;
	padding:35px 0 0 3px;
}

#top_bot
{
	width:980px;
	height:50px;
	float:left;
}
#search
{
	width:450px;
	height:35px;
	float:right;
	margin:10px 0 0 15px;
	background:#2E85EA;
	border-radius:5px;
}

input.freee
{
	width:220px;
	height:21px;
	float:left;
	padding:3px 3px 3px 8px;
	margin:4px 0 5px 4px;
	font-size:12px;
	outline:none;
	color:#7A7A7A;
	border-radius:5px;
	border:none;
	box-shadow:inset 0px 1px 3px #888;
	font-family:Arial, Helvetica, sans-serif;
}

